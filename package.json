    "gatsby-plugin-pnpm": "^1.2.10",
    "gatsby-plugin-react-helmet": "^6.11.0",
    "gatsby-plugin-remove-fingerprints": "^0.0.2",
    "gatsby-plugin-robots-txt": "^1.8.0",
